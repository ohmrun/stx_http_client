{
  "build" : {
    "-D"  : [
      "no-deprecation-warnings"
    ],
    "-cp" : "src/main/haxe",
    "-lib" : [
      "http-status",
      "tink_core",
      "stx_pico",
      "stx_nano",
      "stx_log",
      "fletcher"
    ],
    "node" : {
      "-lib"  : ["hxnodejs","stx_nodejs"],
      "-main" : "Main",
      "-js"   : "build/node/main.js"
    },
    "js" : {
      "-main" : "Main",
      "-js"   : "build/html/main.js" 
    },
    "neko" : {
      "-main" : "Main",
      "-neko" : "build/neko/neko.n"
    }
  },
  "run" : {
    "neko-test" : [
      ["hx","build","neko"],
      ["neko","build/neko/neko.n"]
    ]
  }
}