{
  "build" : {
    "-D"  : [
      "no-deprecation-warnings"
    ],
    "-cp" : "src/main/haxe",
    "-lib" : [
      "http-status",
      "tink_core",
      "stx_pico",
      "stx_nano",
      "stx_log",
      "fletcher"
    ],
    "main" : {
      "-main" : "Main",
      "node" : {
        "-lib"  : ["hxnodejs","stx_nodejs"],
        "-js"   : "build/node/main.js"
      },
      "js" : {
        "-js"   : "build/html/main.js" 
      },
      "neko" : {
        "-neko" : "build/neko/neko.n"
      },
      "interp" : {
        "--interp" : true
      }
    }
  },
  "run" : {
    "neko-test" : [
      ["hx","build","neko"],
      ["neko","build/neko/neko.n"]
    ]
  }
}